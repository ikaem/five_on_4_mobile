  "version": "0.2.0",
  "configurations": [
    {
      "name": "five_on_4_mobile",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "toolArgs": [
        "--dart-define-from-file",
        ".env" // for env files // make sure prod has its own env file when building
      ]
    },
    {
      "name": "five_on_4_mobile (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "toolArgs": [
        "--dart-define-from-file",
        ".env" // for env files // make sure prod has its own env file when building
      ]
    },
    {
      "name": "five_on_4_mobile (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "toolArgs": [
        "--dart-define-from-file",
        ".env" // for env files // make sure prod has its own env file when building
      ]
    }
  ]
}
